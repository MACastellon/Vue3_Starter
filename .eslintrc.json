{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["eslint:recommended", "plugin:vue/vue3-essential", "plugin:@typescript-eslint/recommended", "plugin:vue/base"],
    "overrides": [],
    "parser": "vue-eslint-parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["vue", "@typescript-eslint"],
    "rules": {
        "semi": "error",
        "quotes": ["error", "single"],
        "vue/multi-word-component-names": 0
    }
}
